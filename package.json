{
	"name": "hookwell-extension",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "npm-watch",
		"lint:build": "move out/_next out/assets &&  sed -i 's/\\/_next/\\/assets/g' out/**.html",
		"build": "next build && next export && npm run lint:build"
	},
	"dependencies": {
		"chrome-types": "^0.1.155",
		"next": "13.0.7",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"typescript": "^4.9.4"
	},
	"devDependencies": {
		"@types/node": "18.11.17",
		"@types/react": "18.0.26",
		"autoprefixer": "^10.4.13",
		"eslint": "8.30.0",
		"eslint-config-next": "13.0.7",
		"npm-watch": "^0.11.0",
		"postcss": "^8.4.20",
		"tailwindcss": "^3.2.4"
	},
	"watch": {
		"build": {
			"patterns": [
				"styles/**",
				"pages/**",
				"public/**",
				"helpers/**",
				"next.config.js"
			],
			"ignore": [
				"out",
				"node_modules",
				".next",
				".vscode"
			],
			"extensions": [
				"js",
				"json"
			]
		}
	}
}
